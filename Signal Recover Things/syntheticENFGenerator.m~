clc, clear, close all;

mu = 60; %Hz
sig = 0.0133;
fs = 441;
framet = 1; % seconds
wlen = floor(framet*fs);

f = randn(1,wlen)+mu * sig;     %gaussian freqs with mean mu and std sig
phi = rand(1,wlen) * 2*pi;      %random phases in U[0,2pi]
enf = filter(1,[1 -0.97],f);

sig = zeros(1,wlen);
t = 1:wlen;
for ii = t
    sinwave = sin(2*pi*f(ii)*t + phi(ii));
    sig = sig + sinwave;
end



figure;plot(enf);

figure;plot(sig);

L = length(sig);
ft = fft(sig);
P2 = abs(ft/L);
P1 = P2(1:round(L/2+1);
P1(2:end-1) = 2*P1(2:end-1);

eff = fs*(0:(L/2))/L;

figure;plot(eff,P1);


